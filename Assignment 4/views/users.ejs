<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="style/users-list.css">
</head>

<%- include('includes/header.ejs') %>

</header>

<main>
    <% if (users.length > 0) { %>
        <div class="list-container">
            <ul>
                <% for (let user of users) { %>
                    <li><%= user %></li>
                <% } %>
            </ul>
        </div>
    <% } else { %>
        <h1>No Users</h1>
    <% } %>
</main>

</body>
</html>